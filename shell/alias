# aliases

### ls ###
alias ls='ls --color'
alias ll='ls -lAFh'
alias l='ls -CF'
alias li='ls -l'
alias la='ls -CAF'
alias lll='ls -lFh'
alias ms=ls
alias sl=ls
alias lq=ls
###

### cd ###
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias cd..='cd ..'
alias u='cd ..'
alias uu='cd ../..'
alias uuu='cd ../../..'
alias uuuu='cd ../../../..'
###

### git ###
alias g='git'
###

### docker ###
alias d='docker'
alias dc='docker-compose'
###